  },
  "homepage": "https://github.com/keithbooher/play_me_the_weather#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "consolidate": "~0.10.0",
    "express": "^4.17.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.9.3",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-oauth": "^1.0.0",
    "passport-spotify": "^1.1.0",
    "swig": "~1.3.2"
